{
  "name": "counter",
  "version": "0.1.0",
  "description": "My module",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "chentsulin/counter",
  "scripts": {
    "start": "node server",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "cross-env NODE_ENV=test babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha -- --recursive --require ./test/setup.js",
    "check": "npm run lint && npm run test ",
    "build": "cross-env NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "author": {
    "name": "C.T. Lin",
    "email": "chentsulin@gmail.com",
    "url": "github.com/chentsulin/redux-boilerplate"
  },
  "devEngines": {
    "node": ">=4"
  },
  "keywords": [
    "redux",
    "counter"
  ],
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cross-env": "^1.0.7",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "fbjs-scripts": "^0.6.0",
    "isparta": "^4.0.0",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^3.0.0-alpha.12",
    "redux-devtools": "^3.2.0",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  }
}
