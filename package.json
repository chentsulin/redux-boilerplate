    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
